{
  "name": "aoi.js",
  "version": "7.0.0",
  "description": "A next generation package to create Discord Bots with built-in string functions.",
  "main": "./dist/cjs/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "browser": "./browser/index.js",
  "scripts": {
    "test": "node tests",
    "start": "node index.js",
    "build": "npx tsc -p tsconfig.esm.json && npx tsc -p tsconfig.cjs.json && npm run createpack",
    "createpack": "node src/pack.js",
    "lint": "eslint . --ext .ts --fix ",
    "install": "npm run build",
    "buildStart": "npm run build && npm start"
  },
  "contributors": [
    "leref",
    "USERSATOSHI"
  ],
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkaruiDevelopment/aoi.js.git"
  },
  "devDependencies": {
    "@types/js-beautify": "^1.13.3",
    "@types/node": "^16.18.23",
    "@types/uglify-js": "^3.17.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "js-beautify": "^1.14.7",
    "typescript": "^5.0.4"
  },
  "type": "commonjs",
  "keywords": [
    "aoi.js",
    "aoi.js-next",
    "aoijs",
    "aoi",
    "typescript",
    "api",
    "bot",
    "discord",
    "discordbot"
  ],
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "@akarui/structures": "github:akaruidevelopment/structures#v2",
    "aoiluna": "github:akaruidevelopment/aoiluna#main",
    "boxen": "^7.1.0",
    "chalk": "^5.2.0",
    "cli-spinners": "^2.9.0",
    "dotenv": "^16.0.3",
    "ora": "^6.3.0",
    "uglify-js": "^3.17.4",
    "undici": "^5.21.0",
    "ws": "^8.13.0"
  },
  "bin": {
    "aoijs": "./bin/index.mjs"
  }
}
